{
	"name": "vscode-live2d",
	"displayName": "vscode-live2d",
	"description": "vscode看板娘",
	"version": "1.6.112",
	"publisher": "CharlesZ",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"icon": "res/logo.png",
	"keywords": [
		"vscode看板娘",
		"看板娘",
		"kanbanniang",
		"live2d",
		"canvas",
		"Rainbow-Fart",
		"Rainbow Fart",
		"fart",
		"彩虹屁",
		"vtuber"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "看板娘配置",
				"type": "Object",
				"properties": {
					"vscode-live2d.enabled": {
						"type": "boolean",
						"default": true,
						"description": "是否启用看板娘"
					},
					"vscode-live2d.model": {
						"type": "string",
						"enum": [
							"shizuku",
							"shizuku-pajama",
							"bilibili-22",
							"bilibili-33",
							"Pio",
							"Tia",
							"noir",
							"nepnep",
							"nepmaid",
							"nepgear",
							"vert",
							"tororo",
							"hijiki"
						],
						"default": "shizuku",
						"description": "选哪个娘"
					},
					"vscode-live2d.modelUrl": {
						"type": "string",
						"default": "",
						"description": "自定义导入喜欢的模型url，错误的url会导致看板娘无法正常显示"
					},
					"vscode-live2d.modelWidth": {
						"type": "integer",
						"maximum": 1000,
						"minimum": 50,
						"default": 280,
						"description": "看板娘宽度 default: 280"
					},
					"vscode-live2d.modelHeight": {
						"type": "integer",
						"maximum": 1000,
						"minimum": 50,
						"default": 250,
						"description": "看板娘高度 default: 250"
					},
					"vscode-live2d.moveX": {
						"type": "integer",
						"maximum": 2000,
						"minimum": -1000,
						"default": 0,
						"description": "看板娘水平位置 default: 0"
					},
					"vscode-live2d.moveY": {
						"type": "integer",
						"maximum": 2000,
						"minimum": -1000,
						"default": 0,
						"description": "看板娘垂直位置 default: 0"
					},
					"vscode-live2d.opacity": {
						"type": "number",
						"maximum": 1,
						"minimum": 0.1,
						"default": 1,
						"description": "看板娘透明度"
					},
					"vscode-live2d.pointerOverHidden": {
						"type": "boolean",
						"default": false,
						"description": "鼠标移至上方时隐藏看板娘(会影响看板娘鼠标交互)"
					},
					"vscode-live2d.pointerPenetration": {
						"type": "boolean",
						"default": false,
						"description": "看板娘鼠标穿透(会影响看板娘鼠标交互)"
					},
					"vscode-live2d.position": {
						"type": "string",
						"enum": [
							"right",
							"left"
						],
						"default": "right",
						"description": "看板娘定位"
					},
					"vscode-live2d.talk": {
						"type": "boolean",
						"default": true,
						"description": "看板娘对话气泡"
					}
				}
			}
		]
	},
	"bugs": {
		"url": "https://github.com/iCharlesZ/vscode-live2d/issues",
		"email": "admin@zhchi.me"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CharlesZhang6/vscode-live2d.git"
	},
	"homepage": "https://github.com/iCharlesZ/vscode-live2d/blob/master/README.md",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"vscode:uninstall": "node ./out/uninstall",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
